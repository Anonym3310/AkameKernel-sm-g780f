&panel_drv_0 {
    status = "okay";
	ddi_info = <&s6e3hab>;

	#address-cells = <2>;
	#size-cells = <1>;
	ranges;
	pinctrl-names = "default";
	pinctrl-0 = <&disp_det_ctrl &disp_te_ctrl>;

	gpios {
		disp-reset {
			name = "disp-reset";
			gpios = <&gpa3 4 1>;	/* ACTIVE_LOW */
			dir = <0>;  /* OUT */
			irq-type = <0x0>;   /* NONE */
		};

		disp-det {
			name = "disp-det";
			gpios = <&gpm17 0 1>;  /* 0:OK, 1:NOK(active) */
			dir = <1>;  /* IN */
			irq-type = <0x2>;   /* FALLING EDGE */
			irq-pend {
				reg = <0x0 0x15850A34 0x0>;
				bit = <0x2>;
			};
		};

		err-fg {
			name = "err-fg";
			gpios = <&gpa3 5 0>; 		/* 0:NOK(active) 1:OK */
			dir = <1>;  				/* IN */
			irq-type = <0x00002001>;	/* RISING EDGE | ONE_SHOT */
		};
	};

	regulators {
		ddi-vci {
			regulator = <&dp_ldo4>;
			def-voltage = <3000000>;
			lpm-voltage = <3000000>;
		};

		ddi-vdd3 {
			regulator = <&dp_ldo1>;
			def-voltage = <1800000>;
			lpm-voltage = <1800000>;
		};

		ddr-vddr {
			regulator = <&dp_buck>;
			def-voltage = <1800000>;
			lpm-voltage = <1500000>;
		};

		short-detect {
			regulator = <&dp_elvss>;
			from-off = <2000>;
			from-lpm = <0>;
		};
	};
	panel-lookup {
		panel-name =
			/* OSC:96.5MHz */
			"s6e3hab_hubble1_a3_s0_preliminary_osc_96_5m",
			"s6e3hab_hubble1_a3_s0_default_osc_96_5m",
			"s6e3hab_hubble1_a3_s0_default_osc_96_5m_rev03",
			/* OSC:86.4MHz */
			"s6e3hab_hubble1_a3_s0_preliminary",
			"s6e3hab_hubble1_a3_s0_default",
			"s6e3hab_hubble1_a3_s0_default_rev03";
		panel-ddi-info = <&s6e3hab>;
		panel-display-modes = <&hubble_panel_display_modes>;
		panel-lut = <
			0x000000 0x0000CF 0x0 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV00, OSC:96.5 */
			0x000001 0x0000CF 0x0 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV01, OSC:96.5 */
			0x000002 0x0000CF 0x1 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV02, OSC:96.5 */
			0x000000 0x0000C0 0x2 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV03, OSC:96.5 */

			0x000040 0x0000CF 0x3 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV01, OSC:86.4 */
			0x000041 0x0000CF 0x3 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV01, OSC:86.4 */
			0x000042 0x0000CF 0x4 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV02, OSC:86.4 */
			0x000040 0x0000C0 0x5 0x0  /* S6E3HAB 1440x3200 X1 PANEL REV03, OSC:86.4 */
			0x000000 0x000000 0x2 0x0  /* default */
		>;
	};
};

