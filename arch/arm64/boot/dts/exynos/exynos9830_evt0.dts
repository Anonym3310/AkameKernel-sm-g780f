	/* DMA */
	amba {
		#address-cells = <2>;
		#size-cells = <1>;
		compatible = "arm,amba-bus";
		interrupt-parent = <&gic>;
		ranges;
		pdma0: pdma0@1A510000 {
			compatible = "arm,pl330", "arm,primecell";
			reg = <0x0 0x1A510000 0x1000>;
			interrupts = <GIC_SPI INTREQ__PDMA IRQ_TYPE_LEVEL_HIGH>;
			clocks = <&clock GATE_PDMA>;
			clock-names = "apb_pclk";
			#dma-cells = <1>;
			#dma-channels = <8>;
			#dma-requests = <32>;
			#dma-multi-irq = <1>;
			dma-arwrapper = <0x1A514400>,
					<0x1A514420>,
					<0x1A514440>,
					<0x1A514460>,
					<0x1A514480>,
					<0x1A5144A0>,
					<0x1A5144C0>,
					<0x1A5144E0>;
			dma-awwrapper = <0x1A514404>,
					<0x1A514424>,
					<0x1A514444>,
					<0x1A514464>,
					<0x1A514484>,
					<0x1A5144A4>,
					<0x1A5144C4>,
					<0x1A5144E4>;
			dma-instwrapper = <0x1A514500>;
			dma-mask-bit = <36>;
			coherent-mask-bit = <36>;
		};
	};

