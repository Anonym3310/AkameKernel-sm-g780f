			use_alt_dvfs;
			polling_ms = <0>;
			target_load = "60 676000:84 845000:90 1014000:95 1352000:97 2028000:99";
			min_sample_time = <50>;
			hold_sample_time = <100>;
			hispeed_load = <98>;
			hispeed_freq = <1014000>;
			skew {
				skew_0 {
					constraint_dm_type = <DM_INT>;
					constraint_type = <CONSTRAINT_MIN>;
				};
			};
		};

